{
  "defaultSeverity": "warning",
  "extends": ["tslint:recommended", "tslint-plugin-prettier", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["node_modules/**"]
  },
  "rules": {
    "prettier": true,
    "interface-name": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false
  }
}
